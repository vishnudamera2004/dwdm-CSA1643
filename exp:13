ales<- c(1, 1, 5, 5, 5, 5, 5, 8, 8, 10, 10, 10, 10, 12, 14, 14, 14, 15, 15, 15, 15, 15, 15, 18, 18, 18, 18, 18, 18, 18, 18, 20, 20, 20, 20, 20, 20, 20, 21, 21, 21, 21, 25, 25, 25, 25, 25, 28, 28, 30, 30, 30)
equ_freq_bin<-cut(sales,breaks=3,labels=FALSE)
bin_mean<-tapply(sales,equ_freq_bin,mean)
bin_boundaries<-seq(max(sales),min(sales),length.out=4)
hist(sales,main="histogram",xlab="sales")
print(equ_freq_bin)
print(bin_mean)
print(bin_boundaries)
